   "source": [
    "D0, D1, D2, h, c, F =  symbols('D0, D1, D2, h, c, F')\n",
    "Dd1  = 1/h*(D1-D0)\n",
    "Dd0  = 1/h*(D2-D1)\n",
    "Dd   = Rational(1,2)*(Dd1 + Dd0)\n",
    "Ddd  = 1/h*(Dd1-Dd0)\n",
    "f = Ddd + c*Dd - F\n",
    "solve(f, Dd1)[0]"
   ]
  }
